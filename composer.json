{
    "name": "alexxed/php-junit-merge",
    "type": "library",
    "description": "PHP-Unit J-Unit XML File Merger",
    "homepage": "https://github.com/alexxed/php-junit-merge",
    "license": "MIT",
    "authors": [
        {
            "name": "Andreas Weber",
            "email": "code@andreas-weber.me",
            "role": "lead"
        }
    ],
    "support": {
        "issues": "https://github.com/alexxed/php-junit-merge/issues"
    },
    "require": {
        "php": ">=8.0",
        "symfony/console": "^3.0 || ^4.0 || ^5.0 || ^6.0 || ^7.0",
        "symfony/finder": "^3.0 || ^4.0|| ^5.0 || ^6.0 || ^7.0",
        "theseer/fdomdocument": "1.6.*"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "config": {
        "bin-dir": "vendor/bin/"
    },
    "bin": [
        "phpjunitmerge"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.2",
        "phploc/phploc": "^2.0",
        "squizlabs/php_codesniffer": "^3.10",
        "phpmd/phpmd": "^2.15"
    }
}
